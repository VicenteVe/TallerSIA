        "from google.colab import files\n",
        "\n",
        "# Función para formatear los datos\n",
        "def formatear_datos(input_file):\n",
        "    # Leer el archivo de texto\n",
        "    with open(input_file, 'r') as f:\n",
        "        # Leer líneas y formatear cada línea\n",
        "        lines = [line.strip() for line in f.readlines()]\n",
        "        # Formatear cada línea separada por coma y entre comillas simples\n",
        "        formatted_lines = [f\"'{line}'\" for line in lines]\n",
        "\n",
        "    # Imprimir los datos formateados\n",
        "    print(','.join(formatted_lines))\n",
        "\n",
        "# Cargar el archivo de texto desde el sistema local\n",
        "uploaded = files.upload()\n",
        "\n",
        "# Obtener el nombre del archivo cargado\n",
        "input_file = list(uploaded.keys())[0]\n",
        "\n",
        "# Llamar a la función para formatear los datos y luego imprimirlos\n",
        "formatear_datos(input_file)"
      ],
      "metadata": {
        "id": "cqTcgqab7gT-",
